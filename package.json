{
  "name": "web-wombat",
  "version": "1.13.0",
  "description": "Framework for web services.",
  "main": "src/index.js",
  "scripts": {
    "e2e-test": "node tests/endToEndTests/test.js",
    "unit-test": "mocha tests/unitTests/",
    "test": "npm run unit-test",
    "prettify": "prettier --trailing-coma --write --use-tabs --tab-width 4 --arrow-parens always \"**/*.js\"",
    "code-coverage": "nyc npm run unit-test",
    "code-coverage-strict": "nyc --exclude=tests/unitTests --check-coverage --statements=65 --branches=55 --functions=65 --lines=65 npm run unit-test",
    "code-coverage-todo": "nyc --exclude=tests/unitTests --check-coverage --statements=65 --branches=55 --functions=65 --lines=65 --skip-full npm run unit-test",
    "coveralls": "nyc mocha tests/unitTests/ && nyc report --reporter=text-lcov > ./lcov.info"
  },
  "bin": {
    "web-wombat": "cli/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kukko/web-wombat.git"
  },
  "author": "kukko <nyariag@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kukko/web-wombat/issues"
  },
  "homepage": "https://github.com/kukko/web-wombat#readme",
  "keywords": [
    "web",
    "framework",
    "rest",
    "restful",
    "app",
    "api",
    "websocket",
    "wombat"
  ],
  "dependencies": {
    "bcrypt": "^5.0.0",
    "blade": "^3.3.1",
    "cookie": "^0.4.1",
    "formidable": "^1.2.1",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.4",
    "mongodb": "^3.5.2",
    "mustache": "^3.2.1",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cheerio": "^1.0.0-rc.3",
    "coveralls": "^3.1.0",
    "form-data": "^3.0.0",
    "mocha": "^8.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0",
    "prettier": "^1.19.1",
    "proxyquire": "^2.1.3",
    "request": "^2.88.2",
    "sinon": "^7.5.0",
    "websocket": "^1.0.32"
  }
}
